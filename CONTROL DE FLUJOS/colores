## Se separa con comas y entre corchetes []
#vocales = ['a','e','i','o','u']
#for vocal in vocales:
# print(vocal)
colores = ['amarillo','rojo','azul','marron','anaranjado']
clr = input('ingrese color :')
for color in colores:
 if color == clr :
   print(color)
   print(f"Se encontro el color:{color}")
   break
print(color)